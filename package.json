  },
  "homepage": "https://github.com/artdecocode/catchment#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-rename-import": "2.1.1",
    "babel-preset-env": "1.6.1",
    "fast-async": "6.3.0",
    "noddy": "1.1.0",
    "npm-run-all": "4.1.2",
    "zoroaster": "0.5.3"
  }
